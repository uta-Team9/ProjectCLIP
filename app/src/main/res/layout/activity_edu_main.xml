<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="team9.clip_loginhomecareer.EduMain"
    android:background="#ffffffff">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="@android:dimen/app_icon_size"
        android:text="View Finance"
        android:id="@+id/view_finance_button"
        android:clickable="true"
        android:onClick="moveViews"
        android:textSize="18dp"
        android:textColor="#ff000000"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@android:dimen/app_icon_size"
        android:text="View Applications"
        android:id="@+id/view_applications_button"
        android:clickable="true"
        android:onClick="moveViews"
        android:textSize="18dp"
        android:textColor="#ff000000"
        android:layout_below="@+id/view_finance_button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <!--android:textColor="#ffffffff"/-->

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@android:dimen/app_icon_size"
        android:text="View Degrees"
        android:id="@+id/view_degrees_button"
        android:clickable="true"
        android:onClick="moveViews"
        android:textSize="18dp"
        android:textColor="#ff000000"
        android:layout_below="@+id/view_applications_button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <!--android:textColor="#ffffffff"/-->



</RelativeLayout>
