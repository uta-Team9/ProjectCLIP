<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="team9.clip_loginhomecareer.EduMain"
    android:background="#ffffffff">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="View Jobs"
        android:id="@+id/view_jobs_button"
        android:padding="16dp"
        android:clickable="true"
        android:onClick="moveViews"
        android:layout_below="@+id/view_goals_button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="18dp"
        android:textColor="#ff000000"/>

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="View Contacts"
        android:id="@+id/view_contacts_button"
        android:padding="16dp"
        android:clickable="true"
        android:layout_below="@+id/view_jobs_button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="moveViews"
        android:textSize="18dp"
        android:textColor="#ff000000"/>
    <!--android:textColor="#ffffffff"/-->

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="View Goals"
        android:id="@+id/view_goals_button"
        android:padding="16dp"
        android:clickable="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="18dp"
        android:onClick="moveViews"
        android:textSize="18dp"
        android:textColor="#ff000000"/>
    <!--android:textColor="#ffffffff"/-->

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="View Online Identities"
        android:id="@+id/view_online_identities_button"
        android:padding="16dp"
        android:clickable="true"
        android:onClick="moveViews"
        android:layout_below="@+id/view_contacts_button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="18dp"
        android:textColor="#ff000000"/>
    <!--android:textColor="#ffffffff"/-->


</RelativeLayout>
